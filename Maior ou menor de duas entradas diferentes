programa {
	funcao inicio() {
		
		inteiro numA
		inteiro numB
		
		
		escreva ("Digite o primeiro numero: " )
		leia(numA)
		
		escreva ("Digite o segundo numero: " )
		leia(numB)

        se ( numA > numB){
        escreva ("o numero " + numA + " é maior")
        
        } 
         senao{
        escreva ("o numero " + numB + " é maior")
         }
		
	}
}
